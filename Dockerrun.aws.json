{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [{
			"name": "api",
			"image": "navrang125/infra-engineer-task_api:latest",
			"essential": true,
			"memory": 512,
			"cpu": 256,
			"portMappings": [{
				"hostPort": 5000,
				"containerPort": 5000
			}]
		},
		{
			"name": "postgres",
			"image": "postgres:alpine",
			"essential": true,
			"memory": 512,
			"cpu": 256,
			"portMappings": [{
				"hostPort": 5432,
				"containerPort": 5432
			}],
			"links": ["postgres"],
			"environment": [{
				"name": "POSTGRES_PASSWORD",
				"value": "3x4mpl3"
			}]


		}
	]
}